{
  "name": "swconcord",
  "displayName": "Swedenborg Concordance Language",
  "description": "Syntax highlighting of Swedenborg Concordance text files",
  "icon": "icon.png",
  "version": "0.2.9",
  "publisher": "new-christian-bible-study",
  "repository": {
    "type": "git",
    "url": "https://github.com/New-Christian-Bible-Study/swconcord-lang-vscode.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [ {
        "id": "swconcord",
        "aliases": [ "Swedenborg Concordance", "swconcord" ],
        "extensions": [ ".swconcord" ],
        "configuration": "./language-configuration.json"
      } ],
    "grammars": [ {
        "language": "swconcord",
        "scopeName": "source.swconcord",
        "path": "./syntaxes/swconcord.tmLanguage.json"
      } ],
    "configurationDefaults": {
      "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": [
              "comment.control.swconcord"
            ],
            "settings": {
              "foreground": "#11e88b"
            }
          },
          {
            "scope": [
              "comment.problem.keyword.swconcord"
            ],
            "settings": {
              "fontStyle": "bold",
              "foreground": "#e81111"
            }
          },
          {
            "scope": [
              "comment.problem.description.swconcord"
            ],
            "settings": {
              "foreground": "#11e88b"
            }
          },
          {
            "scope": [
              "principal_phrase.english.swconcord"
            ],
            "settings": {
              "fontStyle": "bold"
            }
          },
          {
            "scope": [
              "principal_phrase.latin.swconcord"
            ],
            "settings": {
              "fontStyle": "italic"
            }
          },
          {
            "scope": [
              "principal.phrase.xref.swconcord"
            ],
            "settings": {
              "foreground": "#9900ff"
            }
          },
          {
            "scope": [
              "ref.abbrev.swconcord"
            ],
            "settings": {
              "fontStyle": "bold",
              "foreground": "#1180e8"
            }
          },
          {
            "scope": [
              "ref.canonsnc.swconcord"
            ],
            "settings": {
              "fontStyle": "italic bold",
              "foreground": "#1180e8"
            }
          },
          {
            "scope": [
              "bible.ref.swconcord"
            ],
            "settings": {
              "fontStyle": "",
              "foreground": "#1180e8"
            }
          },
          {
            "scope": [
              "latin.injected.swconcord"
            ],
            "settings": {
              "fontStyle": "italic"
            }
          }
        ]
      }
    }
  }
}
